<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    android:layout_margin="20dp">

    <TextView
        android:id="@+id/re_for"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/jiantou1"
        android:text="frozenset({'x10'})"
        android:textColor="@color/gray_3"
        android:textSize="15dp"
        android:layout_marginRight="10dp">

    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="——>"
        android:textColor="@color/gray_3"
        android:id="@+id/jiantou1"
        app:layout_constraintLeft_toRightOf="@id/re_for"
        app:layout_constraintTop_toTopOf="@id/re_for"
        app:layout_constraintRight_toLeftOf="@id/re_for2">

    </TextView>



    <TextView
        android:id="@+id/re_for2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/re_for"
        app:layout_constraintLeft_toRightOf="@id/jiantou1"
        app:layout_constraintRight_toRightOf="parent"
        android:text="frozenset({'x8'})"
        android:textColor="@color/gray_3"
        android:textSize="15dp"
        android:layout_marginLeft="10dp">

    </TextView>

    <TextView
        android:layout_marginTop="10dp"
        android:text="conf: 1.0"
        android:textColor="@color/gray_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/re_conf"
        app:layout_constraintLeft_toLeftOf="@id/re_for"
        app:layout_constraintTop_toBottomOf="@+id/re_for">

    </TextView>

</androidx.constraintlayout.widget.ConstraintLayout>